<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.mathos.jnemcic.mathos">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".MyCourses"
            android:label="My Courses"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".ManageCourses"
            android:label="Manage Courses"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".WebActivity"
            android:label="Webmail"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".UndergraduateStudyProgram"
            android:label="Inženjerstvo"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".EducationStudyProgram"
            android:label="Profesorski"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".FinancialStatisticsProgram"
            android:label="Financijska"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".ComputerScienceProgram"
            android:label="Računarstvo"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
    </application>

</manifest>